#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int p1,p2,q1,q2;
	    cin>>p1>>p2>>q1>>q2;
	    int x = max(p1,p2);
	    int y = max(q1,q2);
	    
	    if(x>y){
	        cout<<"Q"<<endl;
	    }
	    else if(y>x){
	        cout<<"P"<<endl;
	        
	    }
	    else cout<<"TIE"<<endl;
	}
	return 0;
}
